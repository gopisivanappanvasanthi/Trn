@model IEnumerable<Trn.Feature.Students.Models.PapersSub>

<h1>Papers Submitted: </h1>

@foreach (var eachPaper in Model)
{
    <br />
    <div>
        Paper Title : @eachPaper.PaperTitle
    </div>
    <div>
        Paper Description : @eachPaper.PaperDescription
    </div>
    <div>
        Paper Image : @eachPaper.PaperImage
    </div>
    <br />
}
